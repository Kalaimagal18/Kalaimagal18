#include<stdio.h>
void main()
{
for(int i=0;i<5;i++)
 {
  for(int j=0;j<2*i-1;j++)
  {
   printf(" ");
  }
 for(int k=0;k<2*i+1;k++)
 {
  printf("*");
 }
 printf("\n");
 }
for(int i=5;i>=1;i--)
{
 for(int k=5;k<=4;k++)
 {
  printf(" ");
 }
for(int j=2*i-1;j>=1;j--)
 {
  printf("*");
 }
printf(" ");
}
}
