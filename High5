#include<stdio.h>
void main()
{
    char a[100],b[100];
    printf("enter the string: ");
    scanf("%[^\n]",a);
    int k,flag;
    
    for(int i=0;a[i]!='\0';i++)
    {
        flag=0;
        for(int j=0;j<k;j++)
        {
            if(a[i]==b[j] && a[i]!=' ')
            {
                flag=1;
                break;
            }
        }
        if(flag==0)
        {
            b[k++]=a[i];
        }
    }
    b[k]='\0';
    printf("%s",b);
    
}
